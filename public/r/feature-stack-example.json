{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "feature-stack-example",
  "type": "registry:example",
  "title": "Feature Stack Example",
  "registryDependencies": [
    "button",
    "https://shadcraft-registry.vercel.app/r/feature-stack.json"
  ],
  "files": [
    {
      "path": "examples/feature-stack-example.tsx",
      "content": "import { Activity, Check, Mail } from \"lucide-react\";\n\nimport { Button } from \"@/components/ui/button\";\nimport {\n  FeatureStack,\n  FeatureStackAction,\n  FeatureStackBody,\n  FeatureStackContent,\n  FeatureStackHeader,\n  FeatureStackIcon,\n  FeatureStackTitle,\n} from \"@/components/feature-stack\";\n\nexport function FeatureStackExample() {\n  return (\n    <div className=\"flex flex-col gap-16 px-4 py-8 lg:px-8 lg:py-12\">\n      {/* Medium size */}\n      <div className=\"grid gap-16\">\n        <FeatureStack>\n          <FeatureStackIcon icon={<Mail />} variant=\"featured\" />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n\n        <FeatureStack>\n          <FeatureStackIcon icon={<Mail />} />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n\n        <FeatureStack alignment=\"left\">\n          <FeatureStackIcon icon={<Mail />} variant=\"featured\" />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n\n        <FeatureStack alignment=\"left\">\n          <FeatureStackIcon icon={<Mail />} />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n      </div>\n\n      {/* Small size */}\n      <div className=\"grid gap-16\">\n        <FeatureStack size=\"sm\">\n          <FeatureStackIcon icon={<Mail />} variant=\"featured\" />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n\n        <FeatureStack size=\"sm\">\n          <FeatureStackIcon icon={<Mail />} />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n\n        <FeatureStack alignment=\"left\" size=\"sm\">\n          <FeatureStackIcon icon={<Mail />} variant=\"featured\" />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n\n        <FeatureStack alignment=\"left\" size=\"sm\">\n          <FeatureStackIcon icon={<Mail />} />\n          <FeatureStackHeader>\n            <FeatureStackTitle>Email</FeatureStackTitle>\n            <FeatureStackBody>We will respond within 2 business days</FeatureStackBody>\n          </FeatureStackHeader>\n          <FeatureStackAction>\n            <span className=\"text-sm font-medium\">email@example.com</span>\n          </FeatureStackAction>\n        </FeatureStack>\n      </div>\n\n      {/* Examples */}\n      <FeatureStack alignment=\"left\">\n        <FeatureStackIcon icon={<Activity />} variant=\"featured\" />\n        <FeatureStackTitle className=\"text-3xl\">Smarter Analytics</FeatureStackTitle>\n        <FeatureStackBody>\n          Turn data into clarity with AI-powered insights that help you act faster and more\n          confidently.\n        </FeatureStackBody>\n        <FeatureStackContent className=\"flex flex-col gap-2\">\n          {Array.from({ length: 3 }).map((_, index) => (\n            <div key={index} className=\"flex items-center gap-2\">\n              <Check />\n              <p>Feature {index + 1}</p>\n            </div>\n          ))}\n        </FeatureStackContent>\n        <FeatureStackAction className=\"flex gap-2\">\n          <Button>Learn More</Button>\n          <Button variant=\"outline\">Contact Us</Button>\n        </FeatureStackAction>\n      </FeatureStack>\n    </div>\n  );\n}\n",
      "type": "registry:example"
    }
  ],
  "meta": {
    "bundle": "pro-marketing"
  }
}